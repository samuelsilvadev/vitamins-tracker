{
  "name": "vitamins-tracker-server",
  "private": false,
  "version": "0.1.0",
  "description": "Vitamins Tracker Server",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "release": "DISABLE_CZ=true standard-version"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "4.3.8",
    "lint-staged": "^11.2.3",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "@strapi/strapi": "4.1.8",
    "@strapi/plugin-users-permissions": "4.1.8",
    "@strapi/plugin-i18n": "4.1.8",
    "better-sqlite3": "7.4.6"
  },
  "author": {
    "name": "Strapi Team"
  },
  "strapi": {
    "uuid": "17dd7311-f147-4fb4-9331-266b5a49e306"
  },
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "echo 'lint-staged not enabled'",
      "prepare-commit-msg": "[[ -z ${DISABLE_CZ} ]] && exec < /dev/tty && git cz --hook || true"
    }
  },
  "lint-staged": {},
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableSubjectLowerCase": true
    }
  }
}
